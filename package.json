{
  "name": "mf-demo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start:shell": "ng serve shell  -o",
    "start:bulk-imei": "ng serve bulk-imei -o",
    "start:single-imei": "ng serve single-imei -o",
    "build:shell": "ng build shell --prod",
    "build:bulk-imei": "ng build bulk-imei --prod",
    "build:single-imei": "ng build single-imei --prod",
    "start": "concurrently \"npm run start:shell\"  \"npm run start:bulk-imei\" \"npm run start:single-imei\"",
    "build": "npm run build:shell  && npm run build:bulk-imei && npm run build:single-imei",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "run:all": "node node_modules/@angular-architects/module-federation/src/server/mf-dev-server.js"
  },
  "private": true,
  "dependencies": {
    "@angular-architects/module-federation": "^14.0.1",
    "@angular/animations": "^13.1.1",
    "@angular/common": "^13.1.1",
    "@angular/compiler": "^13.1.1",
    "@angular/core": "^13.1.1",
    "@angular/forms": "^13.1.1",
    "@angular/platform-browser": "^13.1.1",
    "@angular/platform-browser-dynamic": "^13.1.1",
    "@angular/router": "^13.1.1",
    "@nrwl/workspace": "^10.3.0",
    "concurrently": "^5.3.0",
    "ngx-build-plus": "^13.0.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "webpack-dev-server": "^4.7.1",
